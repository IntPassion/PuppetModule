{
  "name": "herculesteam-augeasproviders_grub",
  "version": "2.4.0",
  "author": "Dominic Cleal, Raphael Pinson, Trevor Vaughan",
  "summary": "Augeas-based grub types and providers for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/hercules-team/augeasproviders_grub",
  "project_page": "http://augeasproviders.com",
  "issues_url": "https://github.com/hercules-team/augeasproviders_grub/issues",
